<h1>Listing items</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Sku</th>
    <th>Count on hand</th>
    <th>Cost in cents</th>
    <th>Type</th>
    <th>Width</th>
    <th>Height</th>
    <th>Master</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @items.each do |item| %>
  <tr>
    <td><%= item.name %></td>
    <td><%= item.description %></td>
    <td><%= item.sku %></td>
    <td><%= item.count_on_hand %></td>
    <td><%= item.cost_in_cents %></td>
    <td><%= item.type %></td>
    <td><%= item.width %></td>
    <td><%= item.height %></td>
    <td><%= item.master %></td>
    <td><%= link_to 'Show', item %></td>
    <td><%= link_to 'Edit', edit_item_path(item) %></td>
    <td><%= link_to 'Destroy', item, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Item', new_item_path %>
